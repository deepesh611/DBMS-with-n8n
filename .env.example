# n8n Configuration
N8N_ENCRYPTION_KEY=your-encryption-key-here
N8N_USER_MANAGEMENT_JWT_SECRET=your-jwt-secret-here

# MySQL Configuration
MYSQL_ROOT_PASSWORD=rootpassword
MYSQL_DATABASE=church_members
MYSQL_USER=church_user
MYSQL_PASSWORD=church_password

# Note: n8n will create its own 'n8n_db' database automatically
# church_members database will have tables created from assets/church_management.sql

# Ollama Configuration
OLLAMA_HOST=ollama:11434

# Frontend Configuration
# Main webhook for member CRUD operations and bulk import
VITE_N8N_WEBHOOK_URL=<MAIN_WEBHOOK_LINK>
# Image server webhook for serving locally uploaded images
VITE_N8N_IMAGE_URL=<IMAGE_SERVER_WEBHOOK_LINK>